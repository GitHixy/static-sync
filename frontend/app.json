{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/StaticSync BaseLogo.png",
    "extra": {
      "EXPO_API_URL": "https://static-sync.onrender.com"
    },
    
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "splash": {
      "image": "./assets/StaticSync BaseLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "bundleIdentifier": "com.yourapp.staticSync"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/StaticSync BaseLogo.png",
        "backgroundColor": "#ffffff"
      },
      "newArchEnabled": true,
      "package": "com.yourapp.staticSync"
    },
    "web": {
      "favicon": "./assets/StaticSync BaseLogo.png"
    },
    "scheme": "myapp",
    "platforms": ["ios", "android", "web"],
  }
}
